<?xml version="1.0"?>
<extension key="nz.co.fuzion.omnipay" type="payment">
  <file>omnipay</file>
  <name>Omnipay</name>
  <description>Omnipay Card Present Payment Processor</description>
  <license>agpl</license>
  <maintainer>
    <author>Eileen</author>
    <email>eileen@fuzion.co.nz</email>
  </maintainer>
  <releaseDate>2013-06-10</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.2</ver>
    <ver>4.3</ver>
    <ver>4.4</ver>
  </compatibility>
  <comments>This processor in intended to be run in conjunction with the Omnipay app</comments>
  <civix>
    <namespace>CRM/Omnipay</namespace>
  </civix>
    <typeInfo>
    <userNameLabel>username</userNameLabel>
    <passwordLabel>password</passwordLabel>
    <signatureLabel></signatureLabel>
    <subjectLabel></subjectLabel>
    <urlSiteDefault>https://omngateway.net/authorize</urlSiteDefault>
    <urlApiDefault></urlApiDefault>
    <urlRecurDefault></urlRecurDefault>
    <urlSiteTestDefault>https://omngateway.net/authorize</urlSiteTestDefault>
    <urlApiTestDefault>/</urlApiTestDefault>
    <urlRecurTestDefault></urlRecurTestDefault>
    <urlButtonDefault></urlButtonDefault>
    <urlButtonTestDefault></urlButtonTestDefault>
    <billingMode>notify</billingMode>
    <isRecur>0</isRecur>
    <paymentType>1</paymentType>
  </typeInfo>
</extension>
